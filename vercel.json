{
    "version": 2,
    "builds": [
        {
            "src": "build/server/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            },
            "dest": "/build/client/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/build/server/index.js"
        }
    ]
}